{
  "name": "mdprint",
  "version": "0.2.0",
  "private": true,
  "description": "Generates a PDF from one or more Markdown files; handles Nunjucks templating, optional CSS and more.",
  "bin": {
    "mdprint": "./bin/mdprint.js"
  },
  "main": "./lib/index.js",
  "scripts": {
    "test": "semistandard && nyc node test/*.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/attentif/md-to-print-kindling.git"
  },
  "keywords": [
    "book",
    "cli",
    "converter",
    "css",
    "generator",
    "markdown",
    "markdown-it",
    "nunjucks",
    "prince",
    "princexml",
    "print",
    "pdf",
    "renderer",
    "template"
  ],
  "author": "Simon Goumaz <simon@attentif.ch>",
  "license": "MIT",
  "dependencies": {
    "markdown-it": "^12.0.3",
    "nunjucks": "^3.2.2",
    "prince": "^1.8.3",
    "tmp-promise": "^3.0.2",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "coveralls": "^3.1.0",
    "nixt": "^0.5.1",
    "nyc": "^15.1.0",
    "pdf-extraction": "^1.0.2",
    "semistandard": "^16.0.0",
    "tape-async": "^2.3.0"
  }
}
